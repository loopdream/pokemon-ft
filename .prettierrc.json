{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^data/(.*)$",
    "^components/(.*)$",
    "^utils/(.*)$",
    "^[./]"
  ],
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true
}
